import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan= new Scanner(System.in);
        int n=scan.nextInt();
        int sum=0;
        if(n<0) n=n*(-1);
        while(n!=0){
            sum=sum+(n%10);
            n/=10;
        }
        System.out.println(sum);

    }
}
