import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n= scan.nextInt();
        int evensum=0;
        int oddsum=0;
        if(n<0) n=n * (-1);
        while(n!=0){
            int rem=n%10;
            if(rem%2==0) evensum+=rem;
            else oddsum+=rem;
            n=n/10;
        }
        System.out.println(oddsum+" "+evensum);

    }
}
