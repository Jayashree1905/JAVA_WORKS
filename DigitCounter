import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      
        Scanner scan= new Scanner(System.in);
        int n= scan.nextInt();
        int c=0;
        if(n==0) c=1;
        while(n!=0){
            n=n/10;
            c++;
        }
        System.out.println(c);
    }
}
