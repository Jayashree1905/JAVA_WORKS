import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner scan=new Scanner(System.in);
        int n= scan.nextInt();
        int s=0;
        int p=1;
        while(n!=0){
            s=s+(n%10);
            p=p*(n%10);
            n/=10;
        }
        if(s==p)System.out.println("Spy Number");
        else System.out.println("Not a Spy Number");
    }
}
